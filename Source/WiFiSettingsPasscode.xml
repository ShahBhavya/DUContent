<?xml version="1.0" encoding="utf-8" ?>
<uidescription>
  <WiFiSettingsPasscodePage class="Page.Animated" initialFocus="_passcode">

    <Text class="Text.Clock">{Time}</Text>
    <Text class="Text.HeaderTitle" id="_header">#SettingsString</Text>
    <Text class="Text.SubheaderTitle" id="_subheader">#Settings.WiFi.Passcode.Title</Text>

    <VerticalFlowPanel class="VerticalFlowPanel.Menu.Settings">
      <Button class="Button.Vertical" onClick="OnClick_Cancel">#Settings.Cancel</Button>
	  <Button class="Button.Vertical" onClick="OnClick_Back">#Settings.Back</Button>
	  <Button class="Button.Vertical" onClick="OnClick_Next">#Settings.Next</Button>
    </VerticalFlowPanel>

    <VerticalFlowPanel class="VerticalFlowPanel.Content.Settings">
      <Text class="Text.SettingsDescription">#Settings.WiFi.Passcode.Heading</Text>
      <Text class="Text.SettingsDescription" id="_prompt"></Text>
	  <EditText class="EditText.Settings.WiFi.Passcode" id="_passcode"/>
    </VerticalFlowPanel>

    <Image class="Image.Settings.WiFi.Passcode"/>

  </WiFiSettingsPasscodePage>
</uidescription>