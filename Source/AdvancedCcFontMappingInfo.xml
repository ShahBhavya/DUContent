<?xml version="1.0" encoding="utf-8"?>
<uidescription>
  <AdvancedCcFontMappingInfoPage class="Page.Animated" initialFocus="_doneButton">
    <Text class="Text.HeaderTitle" id="_header">#SettingsString</Text>
    <Text class="Text.SubheaderTitle" id="_ccSubHeader">#Settings.ClosedCaption.Advanced.FontMappingInfo.Heading</Text>
    <Text class="Text.Clock">{Time}</Text>

    <VerticalFlowPanel class="VerticalFlowPanel.Menu.Settings">
        <Button class="Button.Vertical" onClick="OnClick_Done" id="_doneButton">#Done</Button>
    </VerticalFlowPanel>

    <VerticalFlowPanel class="VerticalFlowPanel.Content.Settings.List">
      <TextBox class="TextBox.ListPage">#Settings.ClosedCaption.Advanced.FontMappingInfo.Info</TextBox>
    </VerticalFlowPanel>
  </AdvancedCcFontMappingInfoPage>
</uidescription>