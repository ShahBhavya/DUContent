<?xml version="1.0" encoding="utf-8" ?>
<uidescription>
  <RFAddRemoteWizard2 class="Page.Animated.RFAddRemoteWizard">

    <Text class="Text.Clock">{Time}</Text>
    <Text class="Text.HeaderTitle" id="_header">#SettingsString</Text>
    <Text class="Text.SubheaderTitle">#Settings.RfAddRemote.Wizard2</Text>

    <VerticalFlowPanel class="VerticalFlowPanel.Content.Settings.Wide">
      <Text class="Text.SettingsDescription.Body.Long">#Settings.RfAddRemote.Wizard2.Description.Part1</Text>
      <HorizontalFlowPanel height="auto" class="Text.SettingsDescription.Body.Indent">
        <Text class="Text.Settings.RFNumbers">#Settings.RfProgramRemote.Wizard3.ProgrammingPrefixCode.1</Text>
        <Text class="Text.Settings.RFNumbers">#Settings.RfProgramRemote.Wizard3.ProgrammingPrefixCode.2</Text>
        <Text class="Text.Settings.RFNumbers">#Settings.RfProgramRemote.Wizard3.ProgrammingPrefixCode.3</Text>
      </HorizontalFlowPanel>
      <Text class="Text.SettingsDescription.Body.Long">#Settings.RfAddRemote.Wizard2.Description.Part2</Text>
      <HorizontalFlowPanel height="auto" class="Text.SettingsDescription.Body.Indent">
        <Text class="Text.Settings.RFNumbers">{CurrentCode1}</Text>
        <Text class="Text.Settings.RFNumbers">{CurrentCode2}</Text>
        <Text class="Text.Settings.RFNumbers">{CurrentCode3}</Text>
      </HorizontalFlowPanel>
      <Text class="Text.SettingsDescription.Body.Long">#Settings.RfAddRemote.Wizard2.Description.Part3</Text>
    </VerticalFlowPanel>

  </RFAddRemoteWizard2>
</uidescription>