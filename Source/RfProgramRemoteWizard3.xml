<?xml version="1.0" encoding="utf-8" ?>
<uidescription>
  <RFProgramRemoteWizard3 class="Page.Animated">

    <Text class="Text.Clock">{Time}</Text>
    <Text class="Text.HeaderTitle" id="_header">#SettingsString</Text>
    <Text class="Text.SubheaderTitle">#Settings.RfProgramRemote.Wizard3</Text>

    <VerticalFlowPanel class="VerticalFlowPanel.Content.Settings">
      <Text class="Text.SettingsDescription.Title">#Settings.RfProgramRemote.Wizard3.Title</Text>
      <Text class="Text.SettingsDescription.Body.RFRemote">#Settings.RfProgramRemote.Wizard3.Description</Text>
      <HorizontalFlowPanel height="auto" class="Text.SettingsDescription.Body.Indent">
        <Text class="Text.Settings.RFNumbers">#Settings.RfProgramRemote.Wizard3.ProgrammingPrefixCode.1</Text>
        <Text class="Text.Settings.RFNumbers">#Settings.RfProgramRemote.Wizard3.ProgrammingPrefixCode.2</Text>
        <Text class="Text.Settings.RFNumbers">#Settings.RfProgramRemote.Wizard3.ProgrammingPrefixCode.3</Text>
      </HorizontalFlowPanel>
    </VerticalFlowPanel>
    <Text class="Text.SettingsDescription.RFFooter">#Settings.RfProgramRemote.Wizard.Footer</Text>
    <Image class="Image.RfProgramRemote.Stop"/>
  </RFProgramRemoteWizard3>
</uidescription>