<?xml version="1.0" encoding="utf-8" ?>
<uidescription>
  <RFAddRemoteWizard1 class="Page.Animated">

    <Text class="Text.Clock">{Time}</Text>
    <Text class="Text.HeaderTitle" id="_header">#SettingsString</Text>
    <Text class="Text.SubheaderTitle">#Settings.RfAddRemote.Wizard1</Text>

    <VerticalFlowPanel class="VerticalFlowPanel.Menu.Settings">
      <Button class="Button.Vertical" onClick="OnClick_Next" id="_next">#Settings.Next</Button>
      <Button class="Button.Vertical" onClick="OnClick_Cancel" id="_cancel">#Settings.Cancel</Button>
    </VerticalFlowPanel>
    
    <VerticalFlowPanel class="VerticalFlowPanel.Content.Settings">
      <Text class="Text.SettingsDescription.Title">#Settings.RfAddRemote.Wizard1.Title</Text>
      <Text class="Text.SettingsDescription.Body">#Settings.RfAddRemote.Wizard1.Description</Text>
      <Image class="Image.RfProgramRemote.Remote"/>
    </VerticalFlowPanel>
  </RFAddRemoteWizard1>
</uidescription>