<?xml version="1.0" encoding="utf-8" ?>
<uidescription>
    <ParentalControl_ProtectDvrPage class="Page.Animated" initialFocus="_protectDvrOption">

        <Text class="Text.HeaderTitle" id="_header">#SettingsString</Text>
        <Text class="Text.SubheaderTitle">#ParentalControl.Settings.ProtectDvr</Text>
        <Text class="Text.Clock">{Time}</Text>

        <VerticalFlowPanel class="VerticalFlowPanel.Menu.Settings">
            <Button class="Button.Vertical" id="_saveButton" onClick="OnClick_Save">#Settings.Save</Button>
            <Button class="Button.Vertical" id="_dontSaveButton" onClick="OnClick_DontSave">#Settings.DontSave</Button>
        </VerticalFlowPanel>

        <VerticalFlowPanel class="VerticalFlowPanel.Content.Settings.SingleCheckbox">
            <Checkbox class="Lockbox.Settings.ParentalControl" onClick="OnClick_ToggleLock" id="_protectDvrOption">#ParentalControl.Settings.ProtectDvr</Checkbox>
            <Text class="Text.LockAdultPrograms.Description" id="_description">#ParentalControl.Settings.ProtectDvr.Description</Text>
        </VerticalFlowPanel>
    </ParentalControl_ProtectDvrPage>
</uidescription>