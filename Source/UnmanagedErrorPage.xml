<?xml version="1.0" encoding="utf-8" ?>
<uidescription>
  <UnmanagedErrorPage class="Page.Animated">
    <Text class="Text.Clock">{Time}</Text>
    <Text class="Text.HeaderTitle">#UnmanagedErrorPage.ConnectionError</Text>
    <Text class="Text.SubheaderTitle" id="_title"></Text>

    <VerticalFlowPanel class="VerticalFlowPanel.Menu.UnmanagedErrorPage">
      <Button class="Button.Vertical" onClick="OnClick_TryAgain" id="_tryAgain">#UnmanagedErrorPage.TryAgain</Button>
      <Button class="Button.Vertical" href="testnetworkconnection">#UnmanagedErrorPage.DiagnoseNetwork</Button>
    </VerticalFlowPanel>

    <VerticalFlowPanel class="VerticalFlowPanel.Content.UnmanagedErrorPage">
      <Text class="Text.UnmanagedErrorPage">#UnmanagedErrorPage.Content</Text>
      <Text class="Text.UnmanagedErrorPage.ReferURL">#UnmanagedErrorPage.ReferURL</Text>
    </VerticalFlowPanel>
  </UnmanagedErrorPage>
</uidescription>
