<?xml version="1.0" encoding="utf-8" ?>
<uidescription>
  <RFProgramRemoteCancel class="Page.Animated" initialFocus="_tryAgain">

    <Text class="Text.Clock">{Time}</Text>
    <Text class="Text.HeaderTitle">#SettingsString</Text>
    <Text class="Text.SubheaderTitle" id="_subheader"></Text>

    <VerticalFlowPanel class="VerticalFlowPanel.Menu.Settings">
      <Button class="Button.Vertical" onClick="OnClick_TryAgain" id="_tryAgain">#Settings.RfProgramRemote.Button.TryAgain</Button>
      <Button class="Button.Vertical" onClick="OnClick_Cancel" id="_cancel">#Settings.Cancel</Button>
    </VerticalFlowPanel>

    <VerticalFlowPanel class="VerticalFlowPanel.Content.Settings">
      <Text class="Text.SettingsDescription.Title" id="_title"></Text>
      <Text class="Text.SettingsDescription.Body" id="_body"></Text>
    </VerticalFlowPanel>

  </RFProgramRemoteCancel>
</uidescription>