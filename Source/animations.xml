  <!-- 
      OptionPanel animation may now be specified in style/skin.  Thus it makes sense to 
      preserve the original 1.1 animation, provide the new Jackpot animation under a new name, and
      provide a (Jackpot-like) slide from right that meets a specific agreed customer reqt (BT) based
      on earlier Jackpot demos.  The default animation in styles.xml at 1.2 will be 'OptionsInJackpotLight', 
      customized skin.xmls may override.
  -->

  <AnimationCollection class="OptionsInOriginal">
    <Pan class="pan" from="-100.0" dx="0.0" dy="100.0" begin="0.0" duration="0.2" autoReverse="false" loop="false"/>
    <Fade class="fade1" from="0.25" to="1.0" begin="0.0" duration="0.2" repeatDuration="0"/>
  </AnimationCollection>

  <AnimationCollection class="OptionsInJackpotLight">
    <Pan class="pan" from="330.0" dx="-330.0" dy="0" begin="0.0" duration="0.2" autoReverse="false" loop="false"/>
    <Fade class="fade1" from="0.25" to="1.0" begin="0.0" duration="0.2" repeatDuration="0"/>
  </AnimationCollection>

  <AnimationCollection class="OptionsInFromRight">
    <Pan class="pan" from="100.0" dx="-100.0" dy="0.0" begin="0.0" duration="0.2" autoReverse="false" loop="false"/>
